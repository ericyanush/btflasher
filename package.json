{
  "name": "btflasher",
  "version": "0.2.4",
  "description": "Native node module for flashing BrewTroller Boards",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericyanush/btflasher.git"
  },
  "devDependencies": {
    "cmake-js": "^3.4.0",
    "nan": "^2.5.0"
  },
  "dependencies": {
    "node-pre-gyp": "^0.6.32",
    "node-pre-gyp-github": "^1.3.1"
  },
  "binary": {
    "module_name": "btflasher",
    "module_path": "./build/Release",
    "host": "https://github.com/ericyanush/btflasher/releases/",
    "remote_path": "{version}/download/"
  },
  "scripts": {
    "install": "node-pre-gyp reinstall --fallback-to-build=false || cmake-js compile"
  }
